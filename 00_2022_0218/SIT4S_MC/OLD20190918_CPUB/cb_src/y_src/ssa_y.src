	SUB_START
	PUSH_ALL

	.AIF	_CB_CPU_SEL EQ _CB_CPUA

;	===== 2003-02-04 SAITO =====
	FAR_JSR	#_SYUKKA_HANDPLS_SMP_START,R0		; 位置決めＬＳＩから位置取込 手パ カウント開始
;	============================

	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
	FAR_JSR	#_AD_LOAD,R0				;
