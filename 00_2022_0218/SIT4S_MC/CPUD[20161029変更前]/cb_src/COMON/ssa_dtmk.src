;	***	R5[2B] != R6[2B]		***
;	***	THEN R5[2B]==>R6[2B]		***
;	***	      R7 |= R5xorR6		***
;	***					***
;	*******************************************
;	R1~R7
_DATA_CMP2B_R5R6_REF:
	SUB_START
	MOV.W	@R5+,R1				;
	MOV.W	@R6,R2				;
	MOV.W	R1,@R6				;
	ADD	#2,R6				;
	XOR	R2,R1				;
	OR	R1,R7				;違えば0以外
	
	SUB_END
	M_RTS






	.END
