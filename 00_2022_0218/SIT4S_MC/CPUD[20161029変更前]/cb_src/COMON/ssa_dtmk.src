	ADD	#1,R1				;
	MOV.L	R1,@R13				;
	TST	R5,R5				;
	TST_BIT_OF STEP_INF2_MV200		;
	ADD	#-4,R13				;
	ADD	#-4,R14				;
	ADD	#-1,R5				;
	M_BRA	STEP_INF2_MVLP01		;

STEP_INF2_MV200:

