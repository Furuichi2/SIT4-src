;	*******************************************
;	***					***
;	***	ﾃﾞｰﾀ転送 符号拡張		***
;	***	2Bor4B-->2B/4B			***
;	*******************************************
	.MACRO	DATA_STD_SHN_MOV	SRC,L1,DST,L2
	MOV.L	#\SRC,R5		;
	MOV.\L1	@R5,R2			;

	MOV.L	#\DST,R6		;
	MOV.\L2	R2,@R6			;
	.ENDM

;	*******************************************
;	***					***
;	***	ﾃﾞｰﾀ転送 符号拡張		***
;	***	2Bor4B-->2B/4B			***
;	*******************************************
	.MACRO	DATA_MAX_MIN_SHN_MOV	SRC,L1,DST,L2,MAX,L3,MIN,L4
	MOV.L	#\SRC,R5		;
	MOV.\L1	@R5,R2			;
	MOV.\L3	#\MIN,R1		;
	CMP/GE	R1,R2			;
	BT	JMP1\@			;
	MOV	R1,R2			;
JMP1\@:					;
	MOV.\L4	#\MAX,R1		;
	CMP/GE	R2,R1			;
	BT	JMP2\@			;R2=<R1
	MOV	R1,R2			;
JMP2\@:					;
	MOV.L	#\DST,R6		;
	MOV.\L2	R2,@R6			;
	.ENDM



	.MACRO	DATA_LOP_SHN_MOV	SRC,L1,DST,L2,LOOP,CNT_REG,DSTADD
	MOV.W	#\LOOP,\CNT_REG		;
	MOV.L	#\SRC,R5		;
	MOV.L	#\DST,R6		;

JMP0\@:					;
	TST	\CNT_REG,\CNT_REG	;
	TST_BIT_OF JMP1\@		;
	MOV.\L1	@R5+,R2			;

	MOV.\L2	R2,@R6			;
	ADD	#\DSTADD,R6		;
	ADD	#-1,\CNT_REG		;
	M_BRA	JMP0\@			;

JMP1\@:					;
	.ENDM


;	*******************************************
;	***					***
;	***	ｻﾌﾞﾙｰﾁﾝ付転送			***
;	***	ｻﾌﾞﾙｰﾁﾝ内ではR8,R9保持が必要	***
;	***	LOOP=1:1回			***
;	*******************************************
	.MACRO	DATA_LOP_CHG_MOV	SRC,L1,DST,L2,CALLSB,LOOP,CNT_REG,DSTADD
	MOV.W	#\LOOP,\CNT_REG		;
	MOV.L	#\SRC,R8		;
	MOV.L	#\DST,R9		;

JMP0\@:					;
	TST	\CNT_REG,\CNT_REG	;
	TST_BIT_OF JMP1\@		;
	MOV.\L1	@R8+,R2			;

	FAR_JSR	#\CALLSB,R0		;R8,R9保持する事
	
	MOV.\L2	R2,@R9			;
	ADD	#\DSTADD,R9		;
	ADD	#-1,\CNT_REG		;
	M_BRA	JMP0\@			;
JMP1\@:					;
	.ENDM

	.MACRO	DATA_STD_CHG_MOV	SRC,L1,DST,L2,CALLSB
	MOV.L	#\SRC,R8						;
	MOV.\L1	@R8,R2							;

	FAR_JSR	#\CALLSB,R0		;

	MOV.L	#\DST,R9		;
	MOV.\L2	R2,@R9			;
	.ENDM



