	.MACRO	KS_UP_DN_CHK1	SQ_UP,SQ_DN,SET_BIT,SV_UP,SV_DN,LG,LGW_U
	MOV.L	\SQ_UP,R13				;221 UP "*"
	MOV.L	\SQ_DN,R14				;223 DN "*"R0,R13,R14
	MOV.L	\SV_UP,R11				;
