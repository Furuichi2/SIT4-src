;	***************************************************
;	***	ÀΩ√ÿºΩïtÇ´î‰är				***
;	***************************************************
;	å∏è≠ï˚å¸Ç…ÀΩ√ÿºΩÇì¸ÇÍÇÈ[å^äJäÆóπ,ëùà≥äÆóπ]
;	data1  å≥ê›íË
;	data2  [data1-HIS] data
;	IF data1 < PV THEN "1"
;	IF data2 > PV THEN "0"
;	else Keep
	.MACRO	UP1_HIS_DN_PV_ROTL  UP_REG,DN_REG,PV_REG,CF,OLDSQ,OLDBIT,CHKWKREG	;GE or GT
	CMP/\CF	\UP_REG,\PV_REG								;PV >= UP THEN 1
	BT	JMPED\@									;"T=1"
											;
	CMP/\CF	\DN_REG,\PV_REG							;PV >= DN NO THEN "0"
	BF	JMPED\@								;PV < DN

;	--------- KEEP [REG BIT-->TÀﬁØƒ]---------
	MOV.L	#\OLDBIT,\CHKWKREG						;
	TST	\CHKWKREG,\OLDSQ						;"1" THEN T=0
	MOVT	\CHKWKREG							;T-->reg	[îΩì]]
	TST	\CHKWKREG,\CHKWKREG						;REG=0 THEN T=1	[îΩì]]
JMPED\@:									;
	.ENDM									;ANS."T"



;	ëùâ¡ï˚å¸Ç…ÀΩ√ÿºΩÇì¸ÇÍÇÈ[à≥î≤äÆóπ]
;	data1  å≥ê›íË
;	data2  [data1+HIS] data
;	IF data1 > PV THEN "1"
;	IF data2 < PV THEN "0"
;	else Keep

	.MACRO	DN1_HIS_UP_PV_ROTL  UP_REG,DN_REG,PV_REG,CF,OLDSQ,OLDBIT,CHKWKREG	;GE or GT
	CMP/\CF	\UP_REG,\PV_REG							;PV >= UP THEN 1
	BT	JMP1\@								;"T=1" ----> neg "0"

	CMP/\CF	\PV_REG,\DN_REG						;DN >= PV  PV < DN
	BT	JMPED\@							;NO "T=0"  "T=1"

;	--------- KEEP ---------
	MOV.L	#\OLDBIT,\CHKWKREG					;
	TST	\CHKWKREG,\OLDSQ					;"1" THEN T=0
JMP1\@:									;
;	--------- [TÀﬁØƒÇÃîΩì]]---------
	MOVT	\CHKWKREG						;T-->reg	[îΩì]]
	TST	\CHKWKREG,\CHKWKREG					;REG=0 THEN T=1	[îΩì]]
JMPED\@:								;NO "T=0"  "T=1"

	.ENDM									;ANS."T"







