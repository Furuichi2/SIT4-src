;	***						***
;	***************************************************
	.MACRO	MEM_CMP_REG_RL1AD MEM_ADR,WKMEMRG,LG,REGX,CHG,REG1,CF,REG2,ANS_REG,ADD_DT
	MOV.L	#\MEM_ADR,\WKMEMRG			;
	MOV.\LG	@\WKMEMRG,\REGX				;
	.AIF	\CHG EQ	1				;
	EXTU.W	\REGX,\REGX				;
	.AENDI						;
	ADD	\ADD_DT,\REGX				;
	REG1_CMP_REG2_ROTL \REG1,\CF,\REG2,\ANS_REG	;
	.ENDM						;

