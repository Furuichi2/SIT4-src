	EXTU.W	R4,R4					;
	MOV.\LG	@R12,R5					;
	EXTU.W	R5,R5					;
	.AELSE						;
	MOV.\LG	@R11,R4					;
	MOV.\LG	@R12,R5					;
	.AENDI						;

	CMP/GE	R1,R4				;R1[PV] =< R5[SV] UP THEN NOMAL
	BT	KANSHI_020\@			;
	MOV.W	#\SET_BIT,R6			;
	MOV.W	@R13,R0				;R0 WORK
	OR	R6,R0				;PV > SV UP BIT
	MOV.W	R0,@R13				;

	NOT	R6,R6				;
	MOV.W	@R14,R0				;R0 WORK
	AND	R6,R0				;
	MOV.W	R0,@R14				;
	M_BRA	KANSHI_EXT\@			;

KANSHI_020\@:
	CMP/GE	R5,R1			;LIMIT =< PV
	BT	KANSHI_040\@		;NOMAL

	MOV.W	#\SET_BIT,R6		;
	MOV.W	@R14,R0			;R0 WORK
	OR	R6,R0			;
	MOV.W	R0,@R14			;

	NOT	R6,R6			;
	MOV.W	@R13,R0			;R0 WORK
	AND	R6,R0			;
	MOV.W	R0,@R13			;
	M_BRA	KANSHI_EXT\@		;

;	------ NOMAL -----
KANSHI_040\@:				;
	MOV.W	#\SET_BIT,R6		;
	NOT	R6,R6			;
	
	MOV.W	@R13,R0			;R0 WORK
	AND	R6,R0	;
	MOV.W	R0,@R13			;

	MOV.W	@R14,R0			;R0 WORK
	AND	R6,R0		;
	MOV.W	R0,@R14			;

KANSHI_EXT\@:
