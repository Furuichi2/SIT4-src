_IDO_BUF_SRC4B_TOTAL_8B
	SUB_START
	add	r8,r0							;
	mov.l	@r0,r3							;old load
	mov.l	r2,@r0							;
	sub	r3,r2							;

	mov.l	@r5,r3							;total_h LONG
	mov.l	@(4,r5),r4						;total_l LONG
	xor	r1,r1							;
	cmp/pz	r2							;
	bt	IDO_BUF_SRC4B_T8B_100					;
