	.EXPORT	_SMUL1_R1R2_MUL_R5R6_ANS
_SMUL1_R1R2_MUL_R5R6_ANS
	SUB_START
	
	PUSH_REG1 R8		;
	PUSH_REG1 R9		;
	PUSH_REG1 R10		;
	PUSH_REG1 R11		;
	PUSH_REG1 R12		;
	PUSH_REG1 R13		;
	PUSH_REG1 R14		;SHIN

	XOR	R14,R14			;
	CMP/PZ	R1			;
	BT	SMUL1_8B_M_8B100	
	NOT	R14,R14			;符号　"-1"

	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R0

SMUL1_8B_M_8B100

	CMP/PZ	R5			;
	BT	SMUL1_8B_M_8B200	;
	NOT	R14,R14			;符号　"-1"

	NEG1_64	H_REG=R5,L_REG=R6,WKREG=R0


SMUL1_8B_M_8B200

	DMULU.L	R2,R6			;
	STS	MACH,R10		;
	STS	MACL,R11		;

	DMULU.L	R2,R5			;
	STS	MACH,R12		;
	STS	MACL,R13		;

	DMULU.L	R1,R6			;
	STS	MACH,R3			;
	STS	MACL,R0			;

	DMULU.L	R1,R5			;
	STS	MACH,R5			;
	STS	MACL,R6			;

	MOV	R0,R1			;

;	[R5 ,R6 ]
;	    [R3 ,R1]
;	    [R12,R13]
;	        [R10,R11]
;----------------------------
;	[R5 ,R6 ,R1 ,R2 ]
	MOV	R11,R2			;
	XOR	R0,R0			;CY
	XOR	R4,R4			;CY
	CLRT
	ADDC	R10,R13			;
	ADDC	R0,R0			;
	ADDC	R13,R1			;[R1,R2]
	ADDC	R4,R4			;
	ADD	R4,R0			;R0=CY+CY

	XOR	R10,R10			;CY
	XOR	R11,R11			;CY
	XOR	R13,R13			;CY
	CLRT
	ADDC	R3,R6			;
	ADDC	R10,R10			;
	ADDC	R12,R6			;
	ADDC	R11,R11			;
	ADDC	R0,R6			;
	ADDC	R13,R13			;

	ADD	R10,R5
	ADD	R11,R5
	ADD	R13,R5


	TST	R14,R14			;符号　"-1"
	TST_BIT_OF SMUL1_8B_M_8B300	;

	NEG128_R5R6R1R2_WKR8	;R5R6R1R2=>R5R6R1R2


SMUL1_8B_M_8B300

	POP_REG1 R14		;SHIN
	POP_REG1 R13
	POP_REG1 R12
	POP_REG1 R11
	POP_REG1 R10
	POP_REG1 R9
	POP_REG1 R8

	SUB_END
	M_RTS



