;	*******************************************************************
;	***								***
;	***	演算	符号付き 32bit(R2)/32bit(R4)->32bit(R2)		***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	WKREGは保持
;	R2:結果それ以外は保持
	.ALIGN	4
_DIVS_32REG2_32REG1_R4_32REG2_R2:
	SUB_START
	PUSH_REG1	R0
	PUSH_REG1	R1
	MAC_DIVS_32REG2_32REG1_32REG2 REG1=R4,REG2=R2,WKREG1=R0,WKREG2=R1
	POP_REG1	R1
	POP_REG1	R0
	SUB_END
	M_RTS

;	*******************************************************************
;	***								***
;	***	演算	符号付き 32bit(R2)/32bit(R4)->32bit(R2)		***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	WKREGは保持
;	R2:結果それ以外は保持
_DIVS_32REG2_32REG1_R4_32REG2_R2_MOD_R1:
	SUB_START
	PUSH_REG1	R2
	PUSH_REG1	R4
	MAC_DIVS_32REG2_32REG1_32REG2 REG1=R4,REG2=R2,WKREG1=R0,WKREG2=R1

	MOV	R2,R0					;R0 SAVE
	POP_REG1 R4
	DMULS.L	R2,R4					;R2*R1()
	STS	MACL,R4					;
	POP_REG1 R1					;
	SUB	R4,R1					;余り
	MOV	R0,R2					;商
	SUB_END
	M_RTS


;	*******************************************************************
;	***								***
;	***	演算	符号付き 32bit(R2)/16bit(R4)->16bit(R2)		***
;	***	演算	符号付き 32bit(R2)/32bit(R4)->32bit(R2)		***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	used r2,r4
_HIGH_DIVS_32REG2_16REG1_R4_16REG2_R2:
	SUB_START
	NEAR_BSR _FPU_DIVS_32REG2_32REG1_R4_32REG2_R2	;
	SUB_END
	M_RTS

;	*******************************************************************
;	***								***
;	***	演算	符号付き 64bit(R1,R2)/32bit(R4)->32bit(R2)	***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	***************************************************
;	***	SHIN R1,R2[64]/R4[32]-->R2[32]		***
;	***************************************************
;	R1~R4 破壊
	.ALIGN	4				;
_DIVS_64R1R2_32R4_32R2:				;
	SUB_START
	PUSH_REG1		R4		;
	PUSH_REG1		R1		;
	CMP/PZ	R4				;
	BT	DIVS6_020			;
	NEG	R4,R4				;
DIVS6_020:
	CMP/PZ	R1				;
	BT	DIVS6_040			;

	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R3

DIVS6_040:
	NEAR_BSR _DIVU_64R1R2_32R4_32R2		;

	MOV.L	#H'7FFFFFFF,R4			;
	CMP/HS	R2,R4				;7FFFFFFF >= data
	BT	DIVS6_060			;
	MOV	R4,R2				;7FFFFFFF >= data
DIVS6_060:					;

	POP_REG1	R1				;
	POP_REG1	R4
	XOR	R4,R1				;"BIT32=0" THEN +
	CMP/PZ	R1				;
	BT	DIVS6_100			;"+"
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R3	;
DIVS6_100:					;
	SUB_END
	M_RTS


;	*******************************************************************
;	***								***
;	***	演算	符号なし 64bit(R1,R2)/32bit(R4)->32bit(R2)	***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	***********************************************************
;	***	UNSHIN	R1,R2[64]/R4[32]-->R2[32]		***
;	***********************************************************
;	USED REG
;	余りなし演算
;	R1~R4破壊
	.ALIGN	4				;
_DIVU_64R1R2_32R4_32R2:			;
	SUB_START
	TST	R4,R4			;
	TST_BIT_OF DIVU4_ZERO_DIV		;ZERO DIV        ANS.MAX
	CMP/HS	R4,R1			;UNSHIN R0 >= R1 ANS.0...R0
	BT	DIVU4_ZERO_DIV		;OVER FLOW
;	============================================
	DIV0U				;FLAG M=Q=T=0

	.arepeat	32		;繰返展開
	ROTCL	R2			;T bit -->
	DIV1	R4,R1			;SHIFT 1 R0-R1 -->Q,T
	.aendr				;

	ROTCL	R2			;T<--R1<--T
	M_BRA	DIVU4_EXT		;

;	============================================
DIVU4_ZERO_DIV:
	XOR	R2,R2			;
	ADD	#H'FF,R2		;H'FFFFFFFF,R2		;MAX SET
DIVU4_EXT:				;
	SUB_END
	M_RTS
	
;	*******************************************
;	***					***
;	***	R1*R3 * R2/R4			***
;	***	R1(8192)*R3(65535)< 4byte	***
;	***					***
;	*******************************************
_MULDIV_R2_MUL_R1_R3_DIV_R4:
	SUB_START
	DMULS.L	R1,R3				;
	STS	MACL,R1				;
	FAR_JSR	#_X_R2_MUL_R1_DIV_R4,R0		;R2*R1/R4= R1,R2/R4 2006-07-31
	SUB_END
	M_RTS


;	***************************************************************************
;	***									***
;	***	演算	符号付き 32bit(R2)*32bit(R1)/16bit(R4)->16bit(R2)	***
;	***		0割り算ﾁｪｯｸ付き						***
;	***									***
;	***************************************************************************
;	used R1,r2,r4
_HIGH_S32R2_MUL_S32R1_DIV_S32R4_S32R2:
	SUB_START
	NEAR_BSR _FPU_R2_MUL_R1_DIV_R4
	SUB_END
	M_RTS




;	*******************************************************************
;	***								***
;	***	演算	符号付き 32bit(R2)/32bit(R4)->32bit(R2)		***
;	***		0割り算ﾁｪｯｸ付き					***
;	***		FPU演算						***
;	*******************************************************************
;	WKREGは保持
;	R2:結果それ以外は保持
;	FPU,FR4,FR2 USED
;	***************************************************
;	***						***
;	***	R2(S32)*R1(S32)/R4(S32)-->R2		***
;	***		余り処理なし			***
;	***		R2とFPU破壊			***
;	***************************************************
;	R2*R1/R4-->R2
_FPU_R2_MUL_R1_DIV_R4:
	SUB_START

	.AIF	_SIT_CPU_SEL EQ _SIT4_CPUB

;	=========７７２７用演算===========
	TST	R4,R4
	TST_BIT_OF FPU_R2_MUL_R1DIVR4_EXT
	PUSH_REG1 R0				;
	FAR_JSR	#_X_R2_MUL_R1_DIV_R4,R0		;R2*R1/R4= R1,R2/R4 2006-07-31
	POP_REG1 R0				;
FPU_R2_MUL_R1DIVR4_EXT:

	.AELSE

	TST	R4,R4
	TST_BIT_OF FPU_R2_MUL_R1DIVR4_EXT

	.AIF	_CB_CPU_SEL EQ	_CB_CPUA
	.AIF	_FPU_RESET	EQ	_COMPILE_YES
	PUSH_REG1 R2
	PUSH_REG1 R4
	STS	FPSCR,R2	;
	MOV.L	#H'00040001,R4	;
	AND	R4,R2		;
	LDS	R2,FPSCR	;
	POP_REG1 R4
	POP_REG1 R2
	.AENDI
	.AENDI

	LDS	R2,FPUL					;
	FLOAT	FPUL,FR2				;
	LDS	R1,FPUL					;
	FLOAT	FPUL,FR1				;
	LDS	R4,FPUL					;
	FLOAT	FPUL,FR4				;
	FMUL	FR1,FR2					;
	FDIV	FR4,FR2					;
	FTRC	FR2,FPUL				;
	STS	FPUL,R2					;
FPU_R2_MUL_R1DIVR4_EXT:

	.AENDI

	SUB_END
	M_RTS

;	***********************************************************
;	***							***
;	***	R2(S32)*R1(S32)*R3(S32)/R4(S32)-->R2(S32)	***
;	***		余り処理なし				***
;	***		R2とFPU破壊				***
;	***********************************************************
;	R2*R1*R3/R4-->R2
_FPU_R2_MUL_R1_R3_DIV_R4:
	SUB_START

	.AIF	_SIT_CPU_SEL EQ _SIT4_CPUB

;	７７２７用演算はここ
	PUSH_REG1 R0					;
	FAR_JSR	#_MULDIV_R2_MUL_R1_R3_DIV_R4,R0		;
	POP_REG1 R0					;

	.AELSE

	TST	R4,R4
	TST_BIT_OF FPU_R2_MUL_R1_R3DIVR4_EXT
	.AIF	_CB_CPU_SEL EQ	_CB_CPUA
	.AIF	_FPU_RESET	EQ	_COMPILE_YES
	PUSH_REG1 R2
	PUSH_REG1 R4
	STS	FPSCR,R2	;
	MOV.L	#H'00040001,R4	;
	AND	R4,R2		;
	LDS	R2,FPSCR	;
	POP_REG1 R4
	POP_REG1 R2
	.AENDI
	.AENDI
	
	LDS	R2,FPUL					;
	FLOAT	FPUL,FR2				;

	LDS	R1,FPUL					;
	FLOAT	FPUL,FR1				;

	LDS	R4,FPUL					;
	FLOAT	FPUL,FR4				;

	FMUL	FR1,FR2					;FR2

	LDS	R3,FPUL					;
	FLOAT	FPUL,FR1				;

	FMUL	FR1,FR2					;FR2

	FDIV	FR4,FR2					;
	FTRC	FR2,FPUL				;
	STS	FPUL,R2					;
FPU_R2_MUL_R1_R3DIVR4_EXT:

	.AENDI

	SUB_END
	M_RTS

;	***************************************************
;	***						***
;	***		R2(S32)/R4(S32)-->R2		***
;	***		余り処理なし			***
;	***		R2とFPU破壊			***
;	***************************************************
;	R2/R4-->R2
_FPU_DIVS_32REG2_32REG1_R4_32REG2_R2:
	SUB_START

	.AIF	_SIT_CPU_SEL EQ _SIT4_CPUB

;	７７２７用演算はここ
	PUSH_REG1 R0
	FAR_JSR	#_DIVS_32REG2_32REG1_R4_32REG2_R2,R0
	POP_REG1 R0
	.AELSE

	TST	R4,R4					;0Div
	TST_BIT_OF FPU_DIVS_32R2_32R1_R4_32R2_R2_EXIT	;
	.AIF	_CB_CPU_SEL EQ	_CB_CPUA
	.AIF	_FPU_RESET	EQ	_COMPILE_YES
	PUSH_REG1 R2
	PUSH_REG1 R4
	STS	FPSCR,R2	;
	MOV.L	#H'00040001,R4	;
	AND	R4,R2		;
	LDS	R2,FPSCR	;
	POP_REG1 R4
	POP_REG1 R2
	.AENDI
	.AENDI
	
	LDS	R2,FPUL					;
	FLOAT	FPUL,FR2				;
	LDS	R4,FPUL					;
	FLOAT	FPUL,FR4				;
	FDIV	FR4,FR2					;FR2/FR4
	FTRC	FR2,FPUL				;
	STS	FPUL,R2					;
FPU_DIVS_32R2_32R1_R4_32R2_R2_EXIT:			;

	.AENDI

	SUB_END
	M_RTS

;	***************************************************
;	***						***
;	***	R2(S32)/R4(S32)-->R2...R1		***
;	***		余り処理有り			***
;	***		R1,R2,R4,MACH,MACLとFPU破壊	***
;	***************************************************
_FPU_DIVS_32REG2_DIV_32REG4_R2_MOD_R1:
	SUB_START

	.AIF	_SIT_CPU_SEL EQ _SIT4_CPUB

;	７７２７用演算はここ
	PUSH_REG1 R0						;
	FAR_JSR	#_DIVS_32REG2_32REG1_R4_32REG2_R2_MOD_R1,R0	;
	POP_REG1 R0						;
	.AELSE

	TST	R4,R4						;0Div
	TST_BIT_OF FPU_DIVS_32R2_DIV32_R4_R2_MOD_R1_EXIT	;
	.AIF	_CB_CPU_SEL EQ	_CB_CPUA
	.AIF	_FPU_RESET	EQ	_COMPILE_YES
	PUSH_REG1 R2
	PUSH_REG1 R4
	STS	FPSCR,R2	;
	MOV.L	#H'00040001,R4	;
	AND	R4,R2		;
	LDS	R2,FPSCR	;
	POP_REG1 R4
	POP_REG1 R2
	.AENDI
	.AENDI
	
	MOV	R2,R1						;
	LDS	R2,FPUL						;
	FLOAT	FPUL,FR2					;
	LDS	R4,FPUL						;
	FLOAT	FPUL,FR4					;
	FDIV	FR4,FR2						;FR2/FR4
	FTRC	FR2,FPUL					;
	STS	FPUL,R2						;
	DMULS.L	R2,R4						;
	STS	MACL,R4						;
	SUB	R4,R1						;
FPU_DIVS_32R2_DIV32_R4_R2_MOD_R1_EXIT:				;

	.AENDI

	SUB_END
	M_RTS


















;	***************************************************
;	***						***
;	***	R2(S32)*R1(S32)/R4(S32)=R1,R2(S64)	***
;	***						***
;	***************************************************
;	Used Reg R1,R2,R3,R4
_MULDIV_R2S32mR1S32dR4S32aR1R2S64:
	SUB_START
	DMULS.L	R1,R2				;
	STS	MACL,R2				;
	STS	MACH,R1				;
	FAR_JSR	#_DIVS_64R1R2_32R4_64R1R2,R3	;
	SUB_END
	M_RTS

;	***********************************************************
;	***							***
;	***	R1,R2(S64)*R3(S32)/R4(S32)=R1,R2(S64)...R3(S32)	***
;	***							***
;	***********************************************************
;	Used Reg R1,R2,R3,R4
;		 R0,R5
;		 (R1,R2*R3)/R4
;	実は余りがR3に入る
;
_MULDIV_R1R2S64mR3S32dR4S32aR1R2S64:
	SUB_START
	MOV	R1,R0
	XOR	R3,R0
	XOR	R4,R0		;演算結果の符合BIT31 SAVE
	PUSH_REG1 R0		;符号SAVE

	CMP/PZ	R1
	BT	MULDIV41_050			;
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R0	;
MULDIV41_050:					;

	CMP/PZ	R3
	BT	MULDIV41_100			;
	NEG	R3,R3				;
MULDIV41_100:					;

	CMP/PZ	R4
	BT	MULDIV41_150			;
	NEG	R4,R4				;
MULDIV41_150:					;

	DMULU.L	R3,R2				;R1,R2*R3
	STS	MACL,R2				;
	STS	MACH,R5				;
	DMULU.L	R3,R1				;
	STS	MACL,R1				;
	STS	MACH,R0				;
;	=== H(R0),M(R1)       ===
;	===       M(R5),L(R2) ===
	XOR	R3,R3
	CLRT
	ADDC	R5,R1				;
	ADDC	R3,R0				;R0,R1,R2
	TST	R0,R0				;
	TST_BIT_OF MULDIV41_200			;
	MOV	#-1,R1				;
	MOV	#-1,R2				;FFFF,FFFF,FFFF,FFFF
MULDIV41_200:					;

;	-- R1,R2/R4 --
	PUSH_REG1	R1			;
	PUSH_REG1	R2			;
	PUSH_REG1	R4			;
	FAR_JSR	#_DIVU_64R1R2_32R4_64R1R2,R0	;(R1,R2)
	POP_REG1	R4			;

	DMULU.L	R4,R2				;R1,R2*R4
	STS	MACL,R6				;
	STS	MACH,R5				;
	DMULU.L	R4,R1				;
	STS	MACL,R4				;MACHは存在しない

	ADD	R4,R5				;R5,R6

	POP_REG1	R3			;
	POP_REG1	R0			;Input(R1,R2)-(R1*R2)*R4=MOD R3
						;=R0,R3-R5,R6
	CLRT
	SUBC	R6,R3				;
	SUBC	R5,R0				;R0,R3(R0=0はないはず)

	POP_REG1	R0				;
	CMP/PZ	R0				;
	BT	MULDIV41_300			;
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R0	;
	NEG	R3,R3				;
MULDIV41_300:					;

	SUB_END
	M_RTS


;	***********************************************************
;	***							***
;	***	R1,R2(S64)*R3(S32)=R1,R2(S64)			***
;	***							***
;	***********************************************************
;	Used Reg R1,R2,R3
;		 R0,R5
;		 R1,R2*R3
;
_MUL_R1R2S64mR3S32aR1R2S64:
	SUB_START
	MOV	R1,R0
	XOR	R3,R0
	PUSH_REG1 R0		;符号SAVE

	CMP/PZ	R1
	BT	MUL41_050			;
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R0	;
MUL41_050:					;

	CMP/PZ	R3
	BT	MUL41_100			;
	NEG	R3,R3				;
MUL41_100:					;

	DMULU.L	R3,R2				;R1,R2*R3
	STS	MACL,R2				;
	STS	MACH,R5				;
	DMULU.L	R3,R1				;
	STS	MACL,R1				;
	STS	MACH,R0				;
;	=== H(R0),M(R1)       ===
;	===       M(R5),L(R2) ===
	XOR	R3,R3
	CLRT
	ADDC	R5,R1				;
	ADDC	R3,R0				;R0,R1,R2
	TST	R0,R0				;
	TST_BIT_OF MUL41_200			;
	MOV	#-1,R1				;
	MOV	#-1,R2				;FFFF,FFFF,FFFF,FFFF
MUL41_200:					;

	POP_REG1	R0			;
	CMP/PZ	R0				;
	BT	MUL41_300			;
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R0	;
	NEG	R3,R3				;
MUL41_300:					;

	SUB_END
	M_RTS


;	*******************************************************************
;	***								***
;	***	演算	符号付き 64bit(R1,R2)/32bit(R4)->64bit(R1,R2)	***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	***************************************************
;	***	SHIN R1,R2[64]/R4[32]-->R1,R2[64]	***
;	***************************************************
;	R1~R4 破壊
	.ALIGN	4				;
_DIVS_64R1R2_32R4_64R1R2:			;
	SUB_START
	PUSH_REG1		R4		;
	PUSH_REG1		R1		;
	CMP/PZ	R4				;
	BT	DIVS16_020			;
	NEG	R4,R4				;
DIVS16_020:
	CMP/PZ	R1				;
	BT	DIVS16_040			;

	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R3

DIVS16_040:
	FAR_JSR #_DIVU_64R1R2_32R4_64R1R2,R3	;

	MOV.L	#H'7FFFFFFF,R4			;
	CMP/HS	R1,R4				;7FFFFFFF >= data
	BT	DIVS16_060			;
	MOV	R4,R1				;7FFFFFFF >= data
	MOV	#H'FFFFFFFF,R2			;7FFF,FFFF,FFFF,FFFF max
DIVS16_060:					;

	POP_REG1	R3			;R3=R1:符号
	POP_REG1	R4			;
	XOR	R4,R3				;"BIT63=0" THEN +
	CMP/PZ	R3				;
	BT	DIVS16_100			;"+"
	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R3	;
DIVS16_100:					;
	SUB_END
	M_RTS


;	*******************************************************************
;	***								***
;	***	演算	符号なし 64bit(R1,R2)/32bit(R4)->32bit(R1,R2)	***
;	***		0割り算ﾁｪｯｸ付き					***
;	***								***
;	*******************************************************************
;	***********************************************************
;	***	UNSHIN	R1,R2[64]/R4[32]-->R1,R2[64]		***
;	***********************************************************
;	USED REG
;	余りなし演算
;	R1~R4破壊
	.ALIGN	4				;
_DIVU_64R1R2_32R4_64R1R2:			;
	SUB_START			
	
	TST	R4,R4					;
	TST_BIT_ON DIVU14_050		;
	M_BRA	DIVU14_ZERO_DIV		;ZERO DIV        ANS.MAX

DIVU14_050:

	MOV	R1,R3		;PUSH_R1
;	====R1,R2=============
	XOR	R0,R0		;R0,R1

	DIV0U				;FLAG M=Q=T=0
	.arepeat	32		;繰返展開
	ROTCL	R1			;T bit -->
	DIV1	R4,R0			;SHIFT 1 R0-R1 -->Q,T
	.aendr				;
	ROTCL	R1			;T<--R1<--T
					;(R0,R1)/R4-->R1
	DMULS.L	R1,R4			;(R1)
	STS	MACL,R6			;
	STS	MACH,R5			;(0),R1/R4-->R1 R1*R4=R5,R6
	XOR	R0,R0			;
	CLRT
	SUBC	R6,R3			;
	SUBC	R5,R0			;

					;R3,R2/R4
	DIV0U				;FLAG M=Q=T=0
	.arepeat	32		;繰返展開
	ROTCL	R2			;T bit -->
	DIV1	R4,R3			;SHIFT 1 R0-R1 -->Q,T
	.aendr				;
	ROTCL	R2			;T<--R1<--T
					;R1,R2
	M_BRA	DIVU14_EXT		;

;	============================================
DIVU14_ZERO_DIV:
	XOR	R2,R2				;
	ADD	#H'FF,R2			;H'FFFFFFFF,R2		;MAX SET
	MOV	R2,R1				;R1,R2=
DIVU14_EXT:					;
	SUB_END
	M_RTS




;	***********************************
;	***				***
;	***	角度演算		***
;	***				***
;	***	0.1度/1度単位		***
;	***				***
;	***********************************
;	Input 	R2(PV)
;		R1(SV1)START
;		R3(SV2)END
;	ANS R0=1 範囲内
;		R0=0

;	=== R1 =< R2 =< R3 THEN R0=1 ELSE R0=0
	.EXPORT	_DIG_AREA_CHK0
_DIG_AREA_CHK0:
	SUB_START
	CMP/GE	R1,R3			;
	BF	DIG_AREA_CK0_100	;0度をはさむ
;	=== 0度をはさなまい===
	XOR	R0,R0
	CMP/GE	R1,R2			;
	BF	DIG_AREA_CK0_200	; PV<SV1
	CMP/GE	R2,R3			;
	BF	DIG_AREA_CK0_200	; SV2<PV2 (-1)
	MOV	#1,R0			;
	M_BRA	DIG_AREA_CK0_200	;

;	==== ０度をはさむ====
DIG_AREA_CK0_100:
	MOV	#1,R0
	CMP/GE	R1,R2
	BT	DIG_AREA_CK0_200	;SV1=<PV=<360
	CMP/GE	R2,R3			;
	BT	DIG_AREA_CK0_200	;
	XOR	R0,R0
DIG_AREA_CK0_200:
	SUB_END
	M_RTS

;
;	***********************************
;	***				***
;	***	角度演算		***
;	***				***
;	***	0.1度/1度単位		***
;	***				***
;	***********************************
;	Input 	R2(PV)
;		R1(SV1)HOLD
;		R3(SV2)OBJ
;	SV1=<~~<SV2 	R5=1
;	SV2==PV2	R5=0
;	SV2<~~=<SV1	R5=-1
;	ANS R5
;
	.EXPORT	_DIG_AREA_CHK1
_DIG_AREA_CHK1:
	SUB_START
	XOR	R5,R5
	CMP/EQ	R2,R3			;
	BT	DIG_AREA_CK1_200	;
	CMP/GE	R1,R3			;
	BF	DIG_AREA_CK1_100	;0度をはさむ
;	=== 0度をはさなまい===
	MOV	#-1,R5			;
	CMP/GE	R1,R2			;
	BF	DIG_AREA_CK1_200	; PV<SV1 (-1)
	CMP/GE	R2,R3			;
	BF	DIG_AREA_CK1_200	; SV2<PV2 (-1)
	MOV	#1,R5
	M_BRA	DIG_AREA_CK1_200	;

;	==== ０度をはさむ====
DIG_AREA_CK1_100:
	MOV	#1,R5
	CMP/GE	R1,R2
	BT	DIG_AREA_CK1_200	;SV1=<PV=<360
	CMP/GE	R2,R3			;
	BT	DIG_AREA_CK1_200	;
	MOV	#-1,R5			;
DIG_AREA_CK1_200:
	SUB_END
	M_RTS




;	*******************************************
;	***					***
;	***					***
;	***		補間１			***
;	***					***
;	***					***
;	*******************************************
;	増加時用  Sn > Sn_1条件(減少方向で使用する場合は入れ替えて使用可能:R6に注意)
;	Input
;	R3:Yn_1		R5:Xn_1
;	R2:Yn		R4:Xn
;	ANS:R2=y	R1:x
;	limit R6(Yn)
;
;
;
	.EXPORT	_FPU_POS_HOKAN1
	.EXPORT	_FPU_TIM_HOKAN1

_FPU_POS_HOKAN1:
	SUB_START
	SUB	R5,R4			;Sn-Sn_1
	SUB	R5,R1			;Sx-Sn_1
	M_BRA	FPU_COM_HOKAN1		;

;	*******************************************
;	***		TIM補間			***
;	*******************************************
;	R3:Yn_1		
;	R2:Yn		R4:Xn - Xn_1
;	ANS:R2=y	R1:x  - Xn_1
;	limit R6(Yn)
;
_FPU_TIM_HOKAN1:
	SUB_START
FPU_COM_HOKAN1:
	TST	R4,R4				;
	TST_BIT_OF COM_HOK_DT_ER_LMT		;R4=0 ERR

	CMP/PL	R4				;R4
	BF	COM_HOK_DT_DN_LMT		;Sn < Sn_1 data err

	CMP/PZ	R1				;R1<0 err
	BF	COM_HOK_DT_DN_LMT		;Yn_1 SET

	CMP/GE	R1,R4				;R1 =< R4 NOMAL
	BF	COM_HOK_END			;R4<R1 THEN ｽｹｰﾙｵｰﾊﾞ Yn(R2)

	SUB	R3,R2				;Yn-Yn_1

;	----------- R2*R1/R4 ==> R2 -------
;;;	FAR_JSR	#_FPU_R2_MUL_R1_DIV_R4,R0	;
	FAR_JSR	#_X_R2_MUL_R1_DIV_R4,R0

	ADD	R3,R2				;
	M_BRA	COM_HOK_END
COM_HOK_DT_ER_LMT:				;Sn=Sn_1
	MOV	R6,R2				;
	M_BRA	COM_HOK_END			;

COM_HOK_DT_DN_LMT:				;Sx<Sn_1
	MOV	R3,R2				;
COM_HOK_END:
	SUB_END
	M_RTS

_X_R2_MUL_R1_DIV_R4:	;
	SUB_START
	PUSH_REG1	R1
	PUSH_REG1	R3
	PUSH_REG1	R4
	PUSH_REG1	R5
	PUSH_REG1	R6
	FAR_JSR	#_MULDIV_R2S32mR1S32dR4S32aR1R2S64,R0	;
	POP_REG1	R6
	POP_REG1	R5
	POP_REG1	R4
	POP_REG1	R3
	POP_REG1	R1
	SUB_END
	M_RTS



