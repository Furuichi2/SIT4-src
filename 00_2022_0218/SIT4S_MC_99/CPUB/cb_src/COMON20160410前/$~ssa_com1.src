;
;	OUT R8:OFFSET POINTER*2
;	ANS r2(total)/BUF-CNT-MAX(R4)
_IDO_BUF_SRC2B_TOTAL_4B_CAL:
	SUB_START

	MOV.W	@R3,R1				;
	MOV	R1,R8				;
