;	input R5,R6,R1,R2/R3,R4

	.MACRO	NEG128_R5R6R1R2_WKR8

	XOR	R8,R8	;

	NOT	R5,R5
	NOT	R6,R6
	NOT	R1,R1
	NOT	R2,R2


	SETT	
	ADDC	R8,R2
	ADDC	R8,R1
	ADDC	R8,R6
	ADDC	R8,R5

	.ENDM




	.ALIGN	4
	.EXPORT	_DIVS1_16B_DIV_8B_ANS_8B		;
_DIVS1_16B_DIV_8B_ANS_8B
	SUB_START
	PUSH_REG1 R7		;LOOP
	PUSH_REG1 R8		;
	PUSH_REG1 R9		;
	PUSH_REG1 R10		;ANS
	PUSH_REG1 R11		;ANS
	PUSH_REG1 R12		;
	PUSH_REG1 R13		;
	PUSH_REG1 R14		;SHIN

	MOV.W	#D'64,R7	;

	XOR	R14,R14		;
	CMP/PZ	R5		;
	BT	DIVS1_16B_DIV_8BANS8B_100
	NOT	R14,R14		;符号　"-1"

	NEG128_R5R6R1R2_WKR8	;R5R6R1R2=>R5R6R1R2

DIVS1_16B_DIV_8BANS8B_100

	CMP/PZ	R3
	BT	DIVS1_16B_DIV_8BANS8B_200
	NOT	R14,R14		;符号　"-1"

	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R8



DIVS1_16B_DIV_8BANS8B_200
	NOP

	XOR	R10,R10		;"H"
	XOR	R0,R0		;"L"

;	------------- ｵｰﾊﾞﾌﾛﾁｪｯｸ---------
	MOV	R5,R12
	MOV	R6,R13
	CLRT
	SUBC	R4,R13				;
	SUBC	R3,R12				;演算結果 "CY=0 引けた"
	BT	DIVS1_16B_DIV_8BANS8B_LOP	;引けなったOK
	NOP

	M_BRA	DIVS1_16B_DIV_8BANS8B_500	;ｵｰﾊﾞﾌﾛ


DIVS1_16B_DIV_8BANS8B_LOP
	SHLL	R2	;
	ROTCL	R1	;
	ROTCL	R6	;
	ROTCL	R5	;R5,R6,R1,R2

	MOV	R5,R12
	MOV	R6,R13

	CLRT
	SUBC	R4,R13				;
	SUBC	R3,R12				;演算結果 "CY=0 引けた"
						;CY=0:引けた　結果を一旦そのまま入れる
	ROTCL	R0				;
	ROTCL	R10				;[R10,R0]
	TST	#BIT0,R0			;
	TST_BIT_ON DIVS1_16B_DIV_8BANS8B_300	;CY=1:引けなかった

	MOV	R12,R5				;CY=0:引けた
	MOV	R13,R6				;引いた結果を反映

DIVS1_16B_DIV_8BANS8B_300

	ADD	#-1,R7				;
	TST	R7,R7				;
	TST_BIT_ON DIVS1_16B_DIV_8BANS8B_LOP	;


	NOT	R10,R10				;CYの反転
	NOT	R0,R0				;


	MOV	R10,R1				;下位8BYTE SAVE
	MOV	R0,R2				;


	TST	R14,R14				;
	TST_BIT_OF DIVS1_16B_DIV_8BANS8B_500

	NEG1_64	H_REG=R1,L_REG=R2,WKREG=R8


DIVS1_16B_DIV_8BANS8B_500


	POP_REG1 R14
	POP_REG1 R13
	POP_REG1 R12
	POP_REG1 R11
	POP_REG1 R10
	POP_REG1 R9
	POP_REG1 R8
	POP_REG1 R7

	SUB_END
	M_RTS



