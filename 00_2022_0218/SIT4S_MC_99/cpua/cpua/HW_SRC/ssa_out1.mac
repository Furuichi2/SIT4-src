;;------ 2015-01-05 XORÇ3âÒé¿çsÇ∑ÇÈ
	.MACRO	_WDT_CLR	WK_REG0,WK_REG1,WK_REG2
	DI_PUSH_SR_SH3	\WK_REG0, \WK_REG1

	MOV.L	#_PDTRB,\WK_REG1
	MOV.W	@\WK_REG1,\WK_REG0	;R0:
	MOV.W	#BIT2,\WK_REG2		;
	XOR	\WK_REG2,\WK_REG0	;
	MOV.W	\WK_REG0,@\WK_REG1	;output

	XOR	\WK_REG2,\WK_REG0	;
	MOV.W	\WK_REG0,@\WK_REG1	;output

	XOR	\WK_REG2,\WK_REG0	;
	MOV.W	\WK_REG0,@\WK_REG1	;output

	EI_POP_SR_SH3 			;
	.ENDM

