	SHLL2	R9				; OP CODE * 4
	MOV.L	#_SEQ_CHG_TBL0,R1		;
	ADD	R9,R1				;

	MOV	R8,R9				;
	SWAP.B	R9,R9				;
	EXTU.B	R8,R8				; R8=ADR
	EXTU.B	R9,R9				; R9=OFFSET

	MOV.L	@R1,R1				; ADDRESS LOAD
