_SEQ_RUN_CHG1_100:
	_WDT_CLR R0,R1,R2			; 
						; R12:’-ªﬁñΩóﬂ∫∞ƒﬁ
						; R12:≤›∏ÿ“›ƒ
						; R13:SEQ-ìWäJñΩóﬂ-WORK 
						; R13:≤›∏ÿ“›ƒ
	CMP/GE	R14,R13				; ‹∞∏“”ÿµ∞ ﬁ¡™Ø∏ IF OVER THEN ERR R13 >= R14
	BT	_SEQ_RUN_CHG1_ERR_END		; R13>=R14 JUMP

	CMP/GE	R6,R12				; º∞π›Ω∫∞ƒﬁóÃàÊ¡™Ø∏ IF OVER THEN ERR R12 >= R6
	BT	_SEQ_RUN_CHG1_ERR_END		; R13>=R14 JUMP

	MOV.W	@R12+,R10			; OP CODE , BIT CODE LOAD
