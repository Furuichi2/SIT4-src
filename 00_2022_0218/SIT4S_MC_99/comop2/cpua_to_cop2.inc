	.MACRO DAT_MOV1 SRC,L1,DST,L2,REG1,REG2,DTREG
	MOV.L	#\SRC,\REG1		;
	MOV.L	#\DST,\REG2		;
	MOV.\L1	@\REG1,\DTREG		;
	MOV.\L2	\DTREG,@\REG2		;
	.ENDM

	.MACRO DAT_MOV1_LOP SRC,L1,DST,L2,REG1,REG2,DTREG,LOPREG,CNT,ADRADD
	MOV.L	#\SRC,\REG1		;
	MOV.L	#\DST,\REG2		;
	MOV.L	#\CNT,\LOPREG

JMPLOP\@:
	TST \LOPREG,\LOPREG		;
	TST_BIT_OF JMP01\@		;
	MOV.\L1	@\REG1+,\DTREG		;
	MOV.\L2	\DTREG,@\REG2		;
	ADD	#-1,\LOPREG		;
	ADD	#\ADRADD,\REG2		;
	M_BRA JMPLOP\@
JMP01\@:
	.ENDM

